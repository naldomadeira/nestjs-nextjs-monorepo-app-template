{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "apps/api/dist/**",
          "node_modules/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/dist/main.js"
    }
  ]
}